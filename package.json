{
	"name": "tabsort",
	"displayName": "Sort Tabs/Open Editors by name",
	"description": "Sorts open tabs and editors by name/path",
	"version": "1.1.0",
	"publisher": "omagerio",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"Sort open tabs",
		"Sort open editors",
		"Sort tabs",
		"Sort editors",
		"Sort"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.2.3",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	},
	"contributes": {
		"configuration": {
			"title": "Sort by full path",
			"properties": {
				"tabsort.sortByFullPath": {
					"type": "boolean",
					"default": true,
					"description": "Use full path instead of file name"
				}
			}
		}
	}
}
